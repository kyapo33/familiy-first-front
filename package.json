{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --ignore-path .eslintignore --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prettier": "prettier --write 'src/**/*.{ts,tsx,md,json}' --config ./.prettierrc",
    "generate-swagger": "rm -rf swagger.json && curl http://localhost:8080/swagger-json -o swagger.json",
    "generate-types": "rm -rf src/schemas && npx swagger-typescript-api -p swagger.json -o src/schemas -n Interfaces.ts",
    "generate": "npm run generate-swagger && npm run generate-types",
    "prepare": "husky install"
  },
  "dependencies": {
    "@capacitor/camera": "^5.0.7",
    "@capacitor/cli": "^5.5.0",
    "@capacitor/core": "^5.5.0",
    "@capacitor/ios": "^5.5.0",
    "@capacitor/keyboard": "^5.0.6",
    "@capacitor/preferences": "^5.0.6",
    "@capacitor/status-bar": "^5.0.6",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/inter": "^5.0.14",
    "@fontsource/roboto": "^5.0.8",
    "@mui/icons-material": "^5.14.14",
    "@mui/joy": "^5.0.0-beta.11",
    "@mui/material": "^5.14.14",
    "@tanstack/react-query": "^5.0.0",
    "axios": "^1.5.1",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "formik": "^2.4.5",
    "formik-wizard-form": "^2.1.0",
    "install": "^0.13.0",
    "lint-staged": "^15.0.1",
    "npm": "^10.2.1",
    "prettier": "^3.0.3",
    "query": "github:tanstack/react-query",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.17.0",
    "react-router-dom": "^6.17.0",
    "swagger-typescript-api": "^13.0.3",
    "vite-plugin-checker": "^0.6.2",
    "zod": "^3.22.4",
    "zod-formik-adapter": "^1.2.0",
    "zustand": "^4.4.3"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.51.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "husky": "^8.0.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-eslint": "^1.8.1"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write 'src/**/*.{ts,tsx,css,md,json}' --config ./.prettierrc"
    ]
  }
}
